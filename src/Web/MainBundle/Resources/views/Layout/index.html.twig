            <!--Content-->  
            <div class="content">
                {% for psg in professionalSkillGroups %}
                <!--bloque content (grupo {{ psg.name }})-->
                    <div class="block-content margBMedium">
                        <!--Skill design-->
                        <div class="skill {{ psg.cssClass }}">
                            <!--Row-->  
                            <div class="row ">
                                <div class="vc">
                                    <div class="col-md-11">
                                        <ul class="skills-list clearfix">
                                            {% for ps in psg.professionalSkills %}
                                                <li>
                                                    <h4>{{ ps.name }}</h4>
                                                    <div class="progress">
                                                        <div class="percentage">
                                                            {{ ps.percentage ~ '%' }}
                                                        </div>
                                                    </div>
                                                </li>
                                            {% endfor %}
                                        </ul>
                                    </div>
                                    <div class="col-md-1 {{ psg.slug }}">
                                        <div class="title {{ psg.cssClass }}">
                                            <span>{{ psg.name }}</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--Fin  row-->
                        </div>
                        <!--Fin  skill design-->
                    </div>
                <!--Fin del bloque content (grupo {{ psg.name }})-->
                {% endfor %}
            </div>
            <!-- ······ Fin Bloque Habilidades Profesionales -->


